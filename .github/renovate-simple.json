{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended"
    ],
    "enabledManagers": [
        "bazel-module"
    ],
    "bazel-module": {
        "enabled": true,
        "fileMatch": [
            "MODULE\\.bazel$"
        ]
    },
    "prConcurrentLimit": 2,
    "branchPrefix": "renovate/",
    "semanticCommits": "enabled",
    "dependencyDashboard": true,
    "dependencyDashboardTitle": "ðŸ¤– HBF Dependency Dashboard"
}
